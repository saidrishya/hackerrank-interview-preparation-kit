/*
A left rotation operation on an array shifts each of the arrayâ€™s elements 1 unit to the left. For example, if 2 left rotations are
performed on array [1,2,3,4,5], then the array would become [3,4,5,1,2].
*/

#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,d;
    cin>>n>>d;
    int a[n],i;
    for(i=0;i<n;i++) cin>>a[i];
    for(i=d;i<n;i++) cout<<a[i]<<" ";
    for(i=0;i<d;i++) cout<<a[i]<<" ";
}
